scoreboard players set @s talked_to_villager_CD 500
execute store result score .INT0 randomNumber run random value 1..9

execute if score .INT0 randomNumber matches 1 run tellraw @s ["",{"selector":"@e[type=minecraft:villager,limit=1,tag=eldrid]"},{"text":": Ah, greetings, traveler. Looking for a map to guide you to distant lands, are you? Well, tell me, where is it you wish to go?"}]
execute if score .INT0 randomNumber matches 2 run tellraw @s ["",{"selector":"@e[type=minecraft:villager,limit=1,tag=eldrid]"},{"text":": Come a bit closer, let me see your face, lad. Now, tell me—where are you coming from, and what path have you chosen to take?"}]
execute if score .INT0 randomNumber matches 3 run tellraw @s ["",{"selector":"@e[type=minecraft:villager,limit=1,tag=eldrid]"},{"text":": Where is it? Where in blazes did it go? I can’t find it! Did I sell it without making a copy? No... no, I couldn’t have. Damn it!"}]
execute if score .INT0 randomNumber matches 4 run tellraw @s ["",{"selector":"@e[type=minecraft:villager,limit=1,tag=eldrid]"},{"text":": I'm old and lazy. It's your turn, to explore the lands."}]
execute if score .INT0 randomNumber matches 5 run tellraw @s ["",{"selector":"@e[type=minecraft:villager,limit=1,tag=eldrid]"},{"text":": These maps, they show the way to forgotten places—ancient ruins and buried kingdoms, the kind of spots most folks’ve long forgotten. Too vague, you say? Nay, they do. And for a gold coin, I’ll hand you one, no questions asked."}]
execute if score .INT0 randomNumber matches 6 run tellraw @s ["",{"selector":"@e[type=minecraft:villager,limit=1,tag=eldrid]"},{"text":": Those who dare to cleanse forgotten structures may find themselves rewarded with rare treasures."}]
execute if score .INT0 randomNumber matches 7 run tellraw @s ["",{"selector":"@e[type=minecraft:villager,limit=1,tag=eldrid]"},{"text":": Have you crossed paths with Faendil Boldtongue yet? He’s the Vogt of Direwood Keep. If ye manage to rid the land of its troubles, bring him proof, and he'll pay you well for your work."}]
execute if score .INT0 randomNumber matches 8 run tellraw @s ["",{"selector":"@e[type=minecraft:villager,limit=1,tag=eldrid]"},{"text":": Greetings! Looking for maps to villages, dungeons and ruins?"}]
execute if score .INT0 randomNumber matches 9 run tellraw @s ["",{"selector":"@e[type=minecraft:villager,limit=1,tag=eldrid]"},{"text":": Always take one step after the other and watch your feet! You never know, where they can carry you."}]
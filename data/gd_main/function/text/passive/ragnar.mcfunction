scoreboard players set @s talked_to_villager_CD 500
execute store result score .INT0 randomNumber run random value 1..12

execute if score .INT0 randomNumber matches 1 run tellraw @s ["",{"selector":"@e[type=minecraft:villager,limit=1,tag=ragnar]"},{"text":": Aye, traveler... ye lookin’ for a bow fit for huntin’, eh? I’ve got what ye need, a fine piece o’ craftin’. No finer in these lands, I swear it on me axe. But mind ye, it ain’t cheap. Quality don’t come free."}]
execute if score .INT0 randomNumber matches 2 run tellraw @s ["",{"selector":"@e[type=minecraft:villager,limit=1,tag=ragnar]"},{"text":": A bow’s good, sharp arrows too. Take both, and ye'll be huntin' like a true huntsman."}]
execute if score .INT0 randomNumber matches 3 run tellraw @s ["",{"selector":"@e[type=minecraft:villager,limit=1,tag=ragnar]"},{"text":": Oh, Dogmeat, ye lovely lad! Come here, then! Aye, that’s it, that’s what ye like, ain’t it? Always knowin’ how to make a man’s day, don’t ye? Good boy."}]
execute if score .INT0 randomNumber matches 4 run tellraw @s ["",{"selector":"@e[type=minecraft:villager,limit=1,tag=ragnar]"},{"text":": Ye lookin' for somethin' special, eh? Well, ye’ve come to the right place. Fire arrows, Ender arrows, Weavin’ arrows... I’ve got it all on stock, all crafted to perfection."}]
execute if score .INT0 randomNumber matches 5 run tellraw @s ["",{"selector":"@e[type=minecraft:villager,limit=1,tag=ragnar]"},{"text":": I ain't one for sellin’ false promises, no. But I’ll tell ye this—what I’ve got here? Better than anythin’ ye’ve got on yer back. Take a look, see for yerself. Maybe somethin’ catches yer fancy."}]
execute if score .INT0 randomNumber matches 6 run tellraw @s ["",{"selector":"@e[type=minecraft:villager,limit=1,tag=ragnar]"},{"text":": Dogmeat! Sit down, lad. Give me yer paw... Now, do a roll. Aye, that’s it! Now that’s what I call good education, eh? Much smarter than most folk around here."}]
execute if score .INT0 randomNumber matches 7 run tellraw @s ["",{"selector":"@e[type=minecraft:villager,limit=1,tag=ragnar]"},{"text":": Aye, too many folk make me skin crawl. Ain’t no peace in a crowd. Just me, Dogmeat, and the quiet of me workshop... that’s all I need."}]
execute if score .INT0 randomNumber matches 8 run tellraw @s ["",{"selector":"@e[type=minecraft:villager,limit=1,tag=ragnar]"},{"text":": What ye be wantin', eh? Speak quick, I’ve got work to be doin'."}]
execute if score .INT0 randomNumber matches 9.. at @p run function gd_main:text/talking_to/ragnar/ragnar_questions